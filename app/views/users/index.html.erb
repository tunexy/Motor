
<%= will_paginate %>

<div class="container">
  <div class="users row">
    <% @users.each do |user| %>
    <div class="panel panel-info">
      <%= link_to user_path(user) do %>
      <div class="panel-body">
        <%= image_tag(user.avatar.url(:medium)) %>
        <p><%= user.user_name %></p>
      </div>
      <% end %>
    </div>
    <% end %>
  </div>
</div>

<%= will_paginate %>